<?xml version="1.0" encoding="UTF-8"?>

<configuration>
    <appenders>
        <console name="console" target="SYSTEM_OUT">
            <patternLayout pattern="%c{1}: %m%n"/>
            <!--patternLayout pattern="%m%n"/-->
        </console>
    </appenders>

    <loggers>
        <root level="warn">
            <appenderRef ref="console"/>
        </root>
    </loggers>
</configuration>
