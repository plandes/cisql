<?xml version="1.0" encoding="UTF-8"?>

<configuration status="off">
    <appenders>
        <console name="console" target="SYSTEM_OUT">
            <patternLayout pattern="%c: %m%n"/>
        </console>
    </appenders>

    <loggers>
        <logger name="zensols.cisql" level="debug"/>
        <root level="warn">
            <appenderRef ref="console"/>
        </root>
    </loggers>
</configuration>
